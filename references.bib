@article{liCNNLSTMMethod2020,
  title = {A {{CNN}}–{{LSTM Method}} for the {{Morphology Evolution Prediction}} of {{Beach Mega-Nourishment}}},
  author = {Li, Yong and Van Oosterom, Peter and Ge, Ying and Zhang, Xiaoxiang and Baart, Fedor},
