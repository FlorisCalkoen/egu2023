@article{lecozannetQuantifyingUncertaintiesSandy2019,
  title = {Quantifying Uncertainties of Sandy Shoreline Change Projections as Sea Level Rises},
  author = {Le Cozannet, Gonéri and Bulteau, Thomas and Castelle, Bruno and Ranasinghe, Roshanka and Wöppelmann, Guy and Rohmer, Jeremy and Bernon, Nicolas and Idier, Déborah and Louisor, Jessie and Salas-y-Mélia, David},
