@article{haasnootGenericAdaptationPathways2019,
  title = {Generic Adaptation Pathways for Coastal Archetypes under Uncertain Sea-Level Rise},
  author = {Haasnoot, Marjolijn and Brown, Sally and Scussolini, Paolo and Jimenez, Jose A and Vafeidis, Athanasios T and Nicholls, Robert J},
  date = {2019-08-01},
  journaltitle = {Environmental Research Communications},
  shortjournal = {Environ. Res. Commun.},
  volume = {1},
