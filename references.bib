@article{caoMappingChangesCoastlines2020,
  title = {Mapping Changes in Coastlines and Tidal Flats in Developing Islands Using the Full Time Series of {{Landsat}} Images},
  author = {Cao, Wenting and Zhou, Yuyu and Li, Rui and Li, Xuecao},
  date = {2020-03},
  journaltitle = {Remote Sensing of Environment},
