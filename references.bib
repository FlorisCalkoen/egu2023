@article{rignotWidespreadRapidGrounding2014,
  title = {Widespread, Rapid Grounding Line Retreat of {{Pine Island}}, {{Thwaites}}, {{Smith}}, and {{Kohler}} Glaciers, {{West Antarctica}}, from 1992 to 2011},
