@article{markusIceCloudLand2017,
  title = {The {{Ice}}, {{Cloud}}, and Land {{Elevation Satellite-2}} ({{ICESat-2}}): {{Science}} Requirements, Concept, and Implementation},
  shorttitle = {The {{Ice}}, {{Cloud}}, and Land {{Elevation Satellite-2}} ({{ICESat-2}})},
  author = {Markus, Thorsten and Neumann, Tom and Martino, Anthony and Abdalati, Waleed and Brunt, Kelly and Csatho, Beata and Farrell, Sinead and Fricker, Helen and Gardner, Alex and Harding, David and Jasinski, Michael and Kwok, Ron and Magruder, Lori and Lubin, Dan and Luthcke, Scott and Morison, James and Nelson, Ross and Neuenschwander, Amy and Palm, Stephen and Popescu, Sorin and Shum, Ck and Schutz, Bob E. and Smith, Benjamin and Yang, Yuekui and Zwally, Jay},
