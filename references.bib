  author = {Almar, Rafael and Bergsma, Erwin W. J. and Thoumyre, Gregoire and Baba, Mohamed Wassim and Cesbron, Guillaume and Daly, Christopher and Garlan, Thierry and Lifermann, Anne},
  date = {2021-01},
  journaltitle = {Remote Sensing},
