@article{honeycuttShorelinePositionForecastingImpact2001,
  title = {Shoreline-{{Position Forecasting}}: {{Impact}} of {{Storms}}, {{Rate-Calculation Methodologies}}, and {{Temporal Scales}}},
  shorttitle = {Shoreline-{{Position Forecasting}}},
  author = {Honeycutt, Maria G. and Crowell, Mark and Douglas, Bruce C.},
  date = {2001},
  journaltitle = {Journal of Coastal Research},
