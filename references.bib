  keywords = {case study,coastal vulnerability,Coastal vulnerability,Combined Coastal Vulnerability Index (CCVI),Fiscal Coastal Vulnerability Index (FCVI),Physical Coastal Vulnerability Index (PCVI),typology,United Kingdom,vulnerability,vulnerability index,vulnerability indices},
