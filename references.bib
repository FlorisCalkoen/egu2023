@article{buscombeReproducibleReusablePipeline2022,
  title = {A {{Reproducible}} and {{Reusable Pipeline}} for {{Segmentation}} of {{Geoscientific Imagery}}},
  author = {Buscombe, D. and Goldstein, E. B.},
  date = {2022},
  journaltitle = {Earth and Space Science},
  volume = {9},
