@article{maoEfficientMeasurementLargeScale2021,
  title = {Efficient {{Measurement}} of {{Large-Scale Decadal Shoreline Change}} with {{Increased Accuracy}} in {{Tide-Dominated Coastal Environments}} with {{Google Earth Engine}}},
  author = {Mao, Yongjing. and Harris, Daniel L. and Xie, Zunyi. and Phinn, Stuart.},
  date = {2021-11},
  journaltitle = {ISPRS Journal of Photogrammetry and Remote Sensing},
