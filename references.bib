@article{kellyBimodalClimateControl2019,
  title = {Bimodal Climate Control of Shoreline Change Influenced by {{Interdecadal Pacific Oscillation}} Variability along the {{Cooloola Sand Mass}}, {{Queensland}}, {{Australia}}},
