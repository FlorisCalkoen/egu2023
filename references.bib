@article{eleveldEstuarineSuspendedParticulate2014,
  title = {Estuarine Suspended Particulate Matter Concentrations from Sun-Synchronous Satellite Remote Sensing: {{Tidal}} and Meteorological Effects and Biases},
  shorttitle = {Estuarine Suspended Particulate Matter Concentrations from Sun-Synchronous Satellite Remote Sensing},
