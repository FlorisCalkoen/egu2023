@article{nichollsSealevelScenariosEvaluating2014,
  title = {Sea-Level Scenarios for Evaluating Coastal Impacts},
  author = {Nicholls, Robert J. and Hanson, Susan E. and Lowe, Jason A. and Warrick, Richard A. and Lu, Xianfu and Long, Antony J.},
