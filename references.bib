@article{almarGlobalAnalysisExtreme2021,
  title = {A Global Analysis of Extreme Coastal Water Levels with Implications for Potential Coastal Overtopping},
  author = {Almar, Rafael and Ranasinghe, Roshanka and Bergsma, Erwin W. J. and Diaz, Harold and Melet, Angelique and Papa, Fabrice and Vousdoukas, Michalis and Athanasiou, Panagiotis and Dada, Olusegun and Almeida, Luis Pedro and Kestenare, Elodie},
  date = {2021-06-18},
