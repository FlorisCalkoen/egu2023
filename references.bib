@article{hallImpactsClimateChange2006,
  title = {Impacts of Climate Change on Coastal Flood Risk in {{England}} and {{Wales}}: 2030–2100},
  shorttitle = {Impacts of Climate Change on Coastal Flood Risk in {{England}} and {{Wales}}},
  author = {Hall, Jim W and Sayers, Paul B and Walkden, Mike J.A and Panzeri, Mike},
