@article{diaz-cuevasDevelopingBeachErosion2020,
  title = {Developing a Beach Erosion Sensitivity Indicator Using Relational Spatial Databases and {{Analytic Hierarchy Process}}},
  author = {Díaz-Cuevas, Pilar and Prieto-Campos, Antonio and Ojeda-Zújar, José},
  date = {2020-05-01},
  journaltitle = {Ocean \& Coastal Management},
