  doi = {10.1002/9781118937099.ch2},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781118937099.ch2},
  abstract = {Summary Storms usually induce a surge that causes the water level to reach a higher position compared to normal conditions. In addition, wave energy is usually higher than normal under storm conditions so that currents and subsequent sediment transport can induce, dramatic morphological changes, including severe erosion. This chapter presents the main processes that control water level variations and hydrodynamic circulation under storm conditions. It reviews the main mechanisms that control storm surges: atmospheric pressure gradients, surface stress and short-wave dissipation. The chapter explores the mechanisms that drive the hydrodynamic circulation along the coast: longshore currents, bed return flows, infragravity waves and swash dynamics. Storm surges correspond to non-astronomic variations in the ocean free-surface driven by meteo-oceanic forcing. The relationship between the height of the storm surge and the bathymetry is explained through the governing shallow water equations, where the wind effect is inversely proportional to the water depth.},
  isbn = {978-1-118-93709-9},
  keywords = {atmospheric pressure gradients,bathymetry,bed return flows,hydrodynamic circulation,infragravity waves,longshore currents,short-wave dissipation,storm conditions,surface stress,swash dynamics}
}

@incollection{doi:https://doi.org/10.1002/9781118937099.ch5,
