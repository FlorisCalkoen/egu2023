  langid = {english},
  keywords = {climate change,Climate change,Climate sciences,climateological change,drivers,Natural hazards,Ocean sciences,ocean warming,wave climate},
  annotation = {Bandiera\_abtest: a Cc\_license\_type: cc\_by Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Climate change;Climate sciences;Natural hazards;Ocean sciences Subject\_term\_id: climate-change;climate-sciences;natural-hazards;ocean-sciences},
