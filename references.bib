@article{almeidaCoastalAnalystSystem2021,
  title = {Coastal {{Analyst System}} from {{Space Imagery Engine}} ({{CASSIE}}): {{Shoreline}} Management Module},
  shorttitle = {Coastal {{Analyst System}} from {{Space Imagery Engine}} ({{CASSIE}})},
